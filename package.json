{
  "name": "counter-lit-element",
  "dependencies": {
    "emotion": "^10.0.27",
    "lit-element": "^2.4.0",
    "normalize.css": "^8.0.1"
  },
  "scripts": {
    "start": "parcel ./src/index.html --open --no-cache",
    "build": "rimraf dist && parcel build ./src/index.html --public-url ./ --no-cache",
    "format": "prettier --write \"src/**/*.js\""
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "babel-plugin-emotion": "^10.0.33",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
